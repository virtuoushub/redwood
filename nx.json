{
  "implicitDependencies": {
    "package.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "lint",
          "test",
          "build"
        ],
        "parallel": 5
      }
    }
  },
  "targetDependencies": {
    "lint": [],
    "test": [],
    "build": []
  },
  "pluginsConfig": {
    "@nrwl/js": {
      "analyzeSourceFiles": false
    }
  }
}
